{
  "name": "express-api",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start:server": "node app.js",
    "start:server:watch": "nodemon app.js",
    "start": "npm-run-all start:server",
    "start:watch": "npm-run-all start:server:watch",
    "dev": "npm-run-all setup-db start:watch",
    "test": "NODE_ENV=test jest --testRunner='jest-circus/runner' --maxWorkers=1",
    "lint": "eslint --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-async-handler": "^1.1.4",
    "faker": "^5.1.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "mysql2": "^2.2.5",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^4.0.2"
  }
}
